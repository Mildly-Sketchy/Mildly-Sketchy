{% extends "base.jinja2" %}
{% block content %}
    <div class="banner">
    </div>
    <h1>Results</h1>
        {% for row in data %}
    <ul id="new-results">
        <li ><pre>{{ row.job_title }}</pre></li>
        <li>{{ row.company }}</li>
        <li>{{ row.location }}</li>
        <li>{{ row.salary }}</li>
        <li>{{ row.job_link }}</li>
    </ul>
        <br>
        {% endfor %}
<form class="sendEmail" action="{{request.route_url('search/email')}}" method="POST">
  <input type="hidden" name="csrf_token" value="{{request.session.get_csrf_token()}}">
  <button>Email Results</button>
</form> 
{% endblock content %}
